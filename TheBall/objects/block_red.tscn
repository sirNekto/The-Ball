script/source = "extends Area3D

var active = false
var time = 0


func _physics_process(delta):
	if active:
		time += delta
		if time > 0.5:
			$Cube001.transform.origin.y -= delta * time * 2
			if time > 6:
				queue_free()

func _on_block_red_body_entered(body):
	if body.name == 'ball':
		active = true
"

